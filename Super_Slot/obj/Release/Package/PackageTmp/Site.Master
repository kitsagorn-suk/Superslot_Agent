    <script>
        var apiURL = '<%=ConfigurationManager.AppSettings["apiURL"] %>';
        $(document).ready(function () {
            $('#divblock').hide();
            $('#txtLan').click(function (e) {
                e.stopPropagation();
                $('#divblock').slideToggle();
            });
            $('#divblock').click(function (e) {
                e.stopPropagation();
            });
            $(document).click(function () {
                $('#divblock').slideUp();
            });

            getProfileSiteMaster();

            if (localStorage.getItem("Position") != "SUPPORT") {
                if (localStorage.getItem("Status_UserOnline") != "OFF") {
                    getOnlineUser();
                }
                else {
                    document.getElementById("ZoneShowOnline").style.display = "none";
                }
            }

            if (localStorage.getItem("Language") == "Thai") {
                $("#txtLan").html("<img src='../img/Flag/thailand.png' style='width: 20px; margin-right: .4rem;' />ไทย");
            }
            else if (localStorage.getItem("Language") == "English") {
                $('#StyleDW_Stament').css('height', '10%');
                $('#StyleDW_Stament_Sup').css('height', '10%');
                $("#txtLan").html("<img src='../img/Flag/usa.png' style='width: 20px; margin-right: .4rem;' />English");
            }
            else if (localStorage.getItem("Language") == "Chinese") {
                $("#txtLan").html("<img src='../img/Flag/china.png' style='width: 20px; margin-right: .4rem;' />中文");
            }

            var m = new Date();
            var dateString = m.getUTCFullYear() + "/" + (m.getUTCMonth() + 1) + "/" + m.getUTCDate() + " " + m.getUTCHours() + ":" + m.getUTCMinutes() + ":" + m.getUTCSeconds();
            var thaiTime = m.toLocaleString("en-US", { timeZone: "Asia/Bangkok" });
            thaiTime = new Date(thaiTime);
            var txtthaiTime = ("0" + thaiTime.getDate()).slice(-2) + "/" + ("0" + (thaiTime.getMonth() + 1)).slice(-2) + "/" + ("0" + thaiTime.getFullYear()).slice(-2) + " " + ("0" + thaiTime.getHours()).slice(-2) + ":" + ("0" + thaiTime.getMinutes()).slice(-2) + ":" + ("0" + thaiTime.getSeconds()).slice(-2);
            $("#timezone").html("<i class='far fa-clock'></i>&nbsp;" + txtthaiTime + " GMT+7");

            if (localStorage.getItem("Position") == "AMB") {
                $('#menuSubUserAdd , #menuSubUserList').css('display', 'none');
            }
            if (localStorage.getItem("Position") == "COMPANY" && localStorage.getItem("UserType") == "main") {
                $('#menuMemberAdd').css('display', 'none');
                $('#menuMarket').css('display', 'block');
            }
            if (localStorage.getItem("Position") == "COMPANY" && localStorage.getItem("UserType") == "sub") {
                $('#menuMemberAdd').css('display', 'none');
                $('#menuMarket').css('display', 'block');
            }
            if (localStorage.getItem("Position") == "SH_HOLDER") {
                $('#menuMemberAdd').css('display', 'none');
            }

            if (localStorage.getItem("Position") == "SUPPORT") {
                document.getElementById("ZoneAll").style.display = "none";
                document.getElementById("ZoneSupport").style.display = "block";
            }
            else {
                document.getElementById("ZoneAll").style.display = "block";
                document.getElementById("ZoneSupport").style.display = "none";
            }

            if (localStorage.getItem("Status_Dashboard") == "OFF") {
                $('#menuDashboard').remove();
            }

            if (localStorage.getItem("Status_Account") == "OFF") {
                $('#menuAccount').remove();
            }
            else if (localStorage.getItem("Status_Account") == "VIEW") {
                $('#menuPassword').remove();
            }

            if (localStorage.getItem("Status_Member") == "OFF") {
                $('#menuMemberManage').remove();
            } else if (localStorage.getItem("Status_Member") == "VIEW") {
                $('#menuShareholderAdd , #menuMemberAdd').remove();
            }

            if (localStorage.getItem("Status_Report") == "OFF") {
                $('#menuReport').remove();
            }

            if (localStorage.getItem("Status_Payment") == "OFF") {
                $('#menuPayment').remove();
            }

            if (localStorage.getItem("Status_Announcement") == "OFF") {
                $('#menuAnnouncement').remove();
            }

            if (localStorage.getItem("Status_Mission") == "OFF") {
                $('#menuMission').remove();
            }

            if (localStorage.getItem("Status_StockManagement") == "OFF") {
                $('#menuCheck').remove();
            }

            if (localStorage.getItem("Status_UserOnline") == "OFF") {
                $('#menuOnlineUser').remove();
            }

            if (localStorage.getItem("Status_FeedBack") == "OFF") {
                $('#menuFeedback').remove();
            }

            if (localStorage.getItem("Status_RandomJackpot") == "OFF" || localStorage.getItem("Status_RandomJackpot") == "") {
                $('#menuRedEnvelope').remove();
            }

            if (localStorage.getItem("Status_Market") == "OFF" || localStorage.getItem("Status_Market") == "") {
                $('#menuMarket').remove();
            }

            if (localStorage.getItem("Status_Notification") == "OFF" || localStorage.getItem("Status_Notification") == "") {
                $('#menuNotification').remove();
            }

            if (localStorage.getItem("Status_Tournament") == "OFF" || localStorage.getItem("Status_Tournament") == "") {
                $('#menuTournament').remove();
            }

            if (localStorage.getItem("Status_DailyReward") == "OFF" || localStorage.getItem("Status_DailyReward") == "") {
                $('#menuDaily').remove();
            }

            if (localStorage.getItem("Status_MarketingTools") == "OFF" || localStorage.getItem("Status_MarketingTools") == "") {
                $('#menuMarketing').remove();
            }

            if (localStorage.getItem("Status_Lotto") == "OFF" || localStorage.getItem("Status_Lotto") == "") {
                $('#menuLucky').remove();
            }

            setInterval(function () {
                var m = new Date();
                var dateString = m.getUTCFullYear() + "/" + (m.getUTCMonth() + 1) + "/" + m.getUTCDate() + " " + m.getUTCHours() + ":" + m.getUTCMinutes() + ":" + m.getUTCSeconds();
                var thaiTime = m.toLocaleString("en-US", { timeZone: "Asia/Bangkok" });
                thaiTime = new Date(thaiTime);
                var txtthaiTime = ("0" + thaiTime.getDate()).slice(-2) + "/" + ("0" + (thaiTime.getMonth() + 1)).slice(-2) + "/" + ("0" + thaiTime.getFullYear()).slice(-2) + " " + ("0" + thaiTime.getHours()).slice(-2) + ":" + ("0" + thaiTime.getMinutes()).slice(-2) + ":" + ("0" + thaiTime.getSeconds()).slice(-2);
                $("#timezone").html("<i class='far fa-clock'></i>&nbsp;" + txtthaiTime + " GMT+7");
            }, 1000);

            SetLan(localStorage.getItem("Language"));
        });

        function SetScrollMaster() {
            var elmnt = document.getElementById("slide-out");
            var y = elmnt.scrollTop;
            if (y != 0 || y > 0) {
                var Check = document.documentElement.clientHeight / 2;
                if (y > Check) {
                    elmnt.scrollTo(0, Check);
                }
                document.getElementById("toggle").style.bottom = "-" + y + "px";
            }
        }

        const getProfileSiteMaster = async () => {
            const requestAwait = await fetchDataSite(`${apiURL}/v1/user/profile`, 'GET', "include", {})
            const response = await requestAwait.json()
            if (response.messageCode == 0000 || response.messageCode == null) {
                localStorage.setItem("Credit", response.data.credit);
                //window.location.href = "/Menu_Dashboard/dashboard.aspx";
                //$("#myModalLoad").modal('hide');
                var txtShow = "";
                document.getElementById('nameLogin').innerHTML = "<i class='far fa-user-circle'></i>" + response.data.username;
                $('#nameLogin').prop('title', response.data.username);
                if (localStorage.getItem("Position") == "AMB") {
                    $("#menuAnnouncement, #menuMission").css("display", "block");
                    txtShow = "AMB";
                    //document.getElementById("AddItem").setAttribute("set-lan", "text:Add Company");
                    document.getElementById("ListItem").setAttribute("set-lan", "text:Company List");
                }
                else if (localStorage.getItem("Position") == "COMPANY" && localStorage.getItem("UserType") == "main") {
                    $("#menuAnnouncement, #menuMission").css("display", "block");
                    txtShow = "Company";

                    if (localStorage.getItem("Status_Member") != "OFF") {
                        document.getElementById("ListItem").setAttribute("set-lan", "text:Shareholder List");
                    }
                    if (localStorage.getItem("Status_Member") == "EDIT") {
                        document.getElementById("AddItem").setAttribute("set-lan", "text:Add Shareholder");
                    }
                    //document.getElementById("AddItem").setAttribute("set-lan", "text:Add Shareholder");
                    //document.getElementById("ListItem").setAttribute("set-lan", "text:Shareholder List");
                }
                else if (localStorage.getItem("Position") == "SH_HOLDER") {
                    txtShow = "Shareholder";

                    if (localStorage.getItem("Status_Member") != "OFF") {
                        document.getElementById("ListItem").setAttribute("set-lan", "text:Agent List");
                    }
                    if (localStorage.getItem("Status_Member") == "EDIT") {
                        document.getElementById("AddItem").setAttribute("set-lan", "text:Add Agent");
                    }
                    //document.getElementById("AddItem").setAttribute("set-lan", "text:Add Agent");
                    //document.getElementById("ListItem").setAttribute("set-lan", "text:Agent List");
                }
                else if (localStorage.getItem("Position") == "AGENT") {
                    $("#menuAnnouncement").css("display", "block");
                    txtShow = "Agent";

                    if (localStorage.getItem("Status_Member") != "OFF") {
                        document.getElementById("ListItem").setAttribute("set-lan", "text:Agents & Members List");
                    }
                    if (localStorage.getItem("Status_Member") == "EDIT") {
                        document.getElementById("AddItem").setAttribute("set-lan", "text:Add Agent");
                    }
                    //document.getElementById("AddItem").setAttribute("set-lan", "text:Add Agent");
                    //document.getElementById("ListItem").setAttribute("set-lan", "text:Agents & Members List");
                }
                else if (localStorage.getItem("Position") == "COMPANY" && localStorage.getItem("UserType") == "sub") {
                    $("#menuAnnouncement, #menuMission").css("display", "block");
                    txtShow = "Member";

                    if (localStorage.getItem("Status_Member") != "OFF") {
                        document.getElementById("ListItem").setAttribute("set-lan", "text:Shareholder List");
                    }
                    if (localStorage.getItem("Status_Member") == "EDIT") {
                        document.getElementById("AddItem").setAttribute("set-lan", "text:Add Shareholder");
                    }
                    //document.getElementById("AddItem").setAttribute("set-lan", "text:Add Shareholder");
                    //document.getElementById("ListItem").setAttribute("set-lan", "text:Shareholder List");
                }
                else if (localStorage.getItem("Position") == "SUPPORT") {
                    txtShow = "Support";
                    document.getElementById("balance").style.display = "none";
                }

                if (localStorage.getItem("UserType") == "sub") {
                    $('#menuSubUserAdd, #menuSubUserList').css('display', 'none');
                }

                document.getElementById('position').innerHTML = "<i class='fas fa-briefcase ee'></i>&nbsp;<span set-lan='text:" + txtShow + "'></span>";
                document.getElementById('balance').innerHTML = "<i class='far fa-money-bill-alt'></i>&nbsp;" + response.data.credit.toString().replace(/(\d)(?=(\d{3})+(?!\d))/g, '$1,');
                SetLan(localStorage.getItem("Language"));
            }
            else if (response.messageCode == 8004) {
                document.getElementById('lbAlert').innerHTML = response.messageDescription;
                $("#myModalLoad").modal('hide');
                $('#modalAlert').modal('show');
                window.location.href = "/Login.aspx";
            }
            else {
                document.getElementById('lbAlert').innerHTML = response.messageDescription;
                $("#myModalLoad").modal('hide');
                $('#modalAlert').modal('show');
            }
        }

        const getOnlineUser = async () => {
            const requestAwait = await fetchDataSite(`${apiURL}/v1/online_user`, 'GET', "include", {})
            const response = await requestAwait.json()
            if (response.messageCode == 0000 || response.messageCode == null) {
                var Sum = response.data.sum;
                if (Sum != 0 || Sum != "" || Sum != null) {
                    $('#txtNumOnline').text(Sum.toString().replace(/(\d)(?=(\d{3})+(?!\d))/g, '$1,'));
                }
                else {
                    $('#txtNumOnline').text("-");
                }
            }
            else if (response.messageCode == 8004) {
                document.getElementById('lbAlert').innerHTML = response.messageDescription;
                $("#myModalLoad").modal('hide');
                $('#modalAlert').modal('show');
                window.location.href = "/Login.aspx";
            }
            else {
                document.getElementById('lbAlert').innerHTML = response.messageDescription;
                $("#myModalLoad").modal('hide');
                $('#modalAlert').modal('show');
            }
        }

        function SetLanguage(ele) {
            SetLan(ele);
            if (ele == "Thai") {
                $("#txtLan").html("<img src='../img/Flag/thailand.png' style='width: 20px; margin-right: .4rem;' />ไทย");
            }
            else if (ele == "English") {
                $('#StyleDW_Stament').css('height', '10%');
                $('#StyleDW_Stament_Sup').css('height', '10%');
                $("#txtLan").html("<img src='../img/Flag/usa.png' style='width: 20px; margin-right: .4rem;' />English");
            }
            else if (ele == "Chinese") {
                $("#txtLan").html("<img src='../img/Flag/china.png' style='width: 20px; margin-right: .4rem;' />中文");
            }

            localStorage.setItem("Language", ele);

            $('#divblock').slideUp();

            var path = window.location.pathname;
            var page = path.split("/").pop();
            if (page == "dashboard") {
                getDashboard();
            }
            else if (page == "Announcements") {
                postAnnounce();
            }
            else if (page == "Mission") {
                GetData();
            }
            else if (page == "game_report") {
                LanSelect();
            }
            else if (page == "tournment_rules") {
                GetDataTR();
            }
        }

        function LogOut() {
            localStorage.clear();
            postLogOut();
        }

        var zoneonline = false;
        function hideOnline() {
            if (zoneonline == true) {
                $("#txtOnline").css("display", "initial");
                zoneonline = false;
            }
            else {
                $("#txtOnline").css("display", "none");
                zoneonline = true;
            }
        }

        function clsAlphaNoOnly(e) {  // Accept only alpha numerics, no special characters 
            var regex = new RegExp("^[a-zA-Z0-9]+$");
            var str = String.fromCharCode(!e.charCode ? e.which : e.charCode);
            if (regex.test(str)) {
                return true;
            }

            e.preventDefault();
            return false;
        }

        function clsAlphaTHNoOnly(e) {  // Accept only alpha numerics, no special characters 
            var regex = new RegExp("^[ก-๏a-zA-Z0-9]+$");
            var str = String.fromCharCode(!e.charCode ? e.which : e.charCode);
            if (regex.test(str)) {
                return true;
            }

            e.preventDefault();
            return false;
        }

        function Comma(Num) { //function to add commas to textboxes
            if (parseInt(Num) != 0) {
                Num += '';
                Num = Num.replace(',', ''); Num = Num.replace(',', ''); Num = Num.replace(',', '');
                Num = Num.replace(',', ''); Num = Num.replace(',', ''); Num = Num.replace(',', '');
                x = Num.split('.');
                x1 = x[0];
                x2 = x.length > 1 ? '.' + x[1] : '';
                var rgx = /(\d+)(\d{3})/;
                while (rgx.test(x1))
                    x1 = x1.replace(rgx, '$1' + ',' + '$2');
                return x1 + x2;
            }
            else if (parseInt(Num) == 0) {
                return 0;
            }
            else {
                return "";
            }
        }

        function isNumberKey(evt) {
            var charCode = (evt.which) ? evt.which : event.keyCode
            if (charCode > 31 && (charCode < 48 || charCode > 57))
                return false;
            return true;
        }

        const fetchDataSite = (url = "", method = "GET", credentials = "include", body = {}) => {
            if (method == "GET") {
                return fetch(url, {
                    method,
                    headers: { 'Content-Type': 'application/json' },
                    credentials,
                })
            }
            return fetch(url, {
                method,
                headers: { 'Content-Type': 'application/json' },
                credentials,
                body: JSON.stringify(body) // body data type must match "Content-Type" header
            })
        }

        const postLogOut = async () => {
            const requestAwait = await fetchDataSite(`${apiURL}/v1/logout`, 'POST', "include", {})
            const response = await requestAwait.json()
            if (response.messageCode == 0000 || response.messageCode == null) {
                window.location.href = "/Login.aspx";
            }
        }

    </script>
    <asp:ContentPlaceHolder ID="MainScript" runat="server"></asp:ContentPlaceHolder>
